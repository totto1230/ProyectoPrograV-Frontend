<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Login1.Vistas.Status"
             Title="Status">

    <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" Spacing="20">
        <BoxView x:Name="circle" Color="Red" WidthRequest="200" HeightRequest="200" CornerRadius="100" />
        <Label x:Name="statuss" Text="Status" FontSize="Large" HorizontalTextAlignment="Center" VerticalTextAlignment="Center" HorizontalOptions="CenterAndExpand"/>
    </StackLayout>

</ContentPage>
